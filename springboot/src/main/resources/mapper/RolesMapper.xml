<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.RolesMapper">
    <insert id="insertRole">
        INSERT INTO roles (username, nickname, password, role)
        VALUES (#{username},#{nickname},#{password},#{role})
    </insert>


    <select id="selectUsername" resultType="com.example.springboot.domain.Roles">
        SELECT * FROM roles WHERE username=#{username}
    </select>
</mapper>
