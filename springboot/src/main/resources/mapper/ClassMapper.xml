<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.ClassMapper">

    <resultMap id="BaseResultMap" type="com.example.springboot.domain.Class">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="className" column="class_name" jdbcType="VARCHAR"/>
            <result property="majorPlanId" column="major_plan_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,class_name,major_plan_id
    </sql>
</mapper>
