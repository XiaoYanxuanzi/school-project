<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.ClassScheduleMapper">

    <select id="findByTime" resultType="com.example.springboot.domain.ClassSchedule">
        SELECT id,section,start_time,end_time FROM class_schedule;
